    "for num in range(1042000,702648265):\n",
    "    order=len(str(num))\n",
    "    sum=0\n",
    "    temp=num\n",
    "    while temp>0:\n",
    "        digit=temp%10\n",
    "        sum=sum+digit**order\n",
    "        temp//=10\n",
    "    if num==sum:\n",
    "        print(num)\n",
    "        break\n",
    "        \n",
    "        \n"
